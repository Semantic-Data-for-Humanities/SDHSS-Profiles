sdh-shacl:sdh-so_C5_Shape a sh:NodeShape ;
    sh:targetClass sdh-so:C5 ;
    rdfs:label "Membership" ;

    sh:property [
        sh:path rdfs:label ;
        sh:datatype xsd:string;
        sh:order 1;
        
        sh:maxCount 1;
        ];

    sh:property [
        sh:path rdfs:comment ;
        sh:datatype rdf:HTML;
        sh:order 2;
        
        ];   

    
        sh:property [
            sh:path sdh-so:P1 ;
            rdfs:label "was a membership of";
            sh:class crm:E21 ;
            sh:maxCount 1 ;
            ];
            
        sh:property [
            sh:path sdh-so:P1 ;
            rdfs:label "was a membership of";
            sh:class crm:E74 ;
            sh:maxCount 1 ;
            ];
            
        sh:property [
            sh:path sdh-so:P2 ;
            rdfs:label "was membership in";
            sh:class crm:E74 ;
            sh:maxCount 1 ;
            ];
            
        sh:property [
            sh:path sdh-so:P3 ;
            rdfs:label "has membership type";
            sh:class sdh-so:C6 ;
            sh:maxCount 1 ;
            ];
            
        sh:property [
            sh:path sdh-so:P63 ;
            rdfs:label "has quality during membership";
            sh:class sdh-so:C2 ;
            
            ];
            
        sh:property [
            sh:path sdh-so:P63 ;
            rdfs:label "has quality during membership";
            sh:class sdh-so:C12 ;
            
            ];
            . 

