    sh:targetClass crm:E67 ;
    rdfs:label 'Birth' ;

    sh:property [
        sh:path rdfs:label ;
        sh:datatype xsd:string;
        sh:order 1;
        
        sh:maxCount 1;
        ];

    sh:property [
        sh:path rdfs:comment ;
        sh:datatype rdf:HTML;
        sh:order 2;
        
        ];   

    
        sh:property [
            sh:path crm:P8 ;
            rdfs:label 'took place on or within';
            sh:class sdh:C17 ;
            
            ];
            
        sh:property [
            sh:path crm:P98 ;
            rdfs:label 'brought into life';
            sh:class crm:E21 ;
            
            ];
            
        sh:property [
            sh:path sdh:P6 ;
            rdfs:label 'took place at';
            sh:class sdh:C13 ;
            
            ];
            . 

