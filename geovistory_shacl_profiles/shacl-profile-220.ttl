        sh:order 2;
        
        ];   
    
    
                sh:property [
                sh:path sdh-short:P1 ;
                sh:name "Standard Date-Time";
                sh:datatype xsd:string;
                sh:maxCount 1;
                sh:order 3;
                ];
                    
                
        sh:property [
                sh:path sdh-so:P43 ;
                sh:name "is acquisition by";
                sh:class crm:E21 ;
                sh:maxCount 1 ;
                ];
                    
        sh:property [
                sh:path sdh-so:P44 ;
