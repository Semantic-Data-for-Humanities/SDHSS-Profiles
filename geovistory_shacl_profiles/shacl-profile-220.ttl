                sh:class crm:E21 ;
                
                ];
                    
        sh:property [
                sh:path sdh-so:P45 ;
                sh:name "issued by";
                sh:class crm:E74 ;
                
                ];
                    
        sh:property [
                sh:path sdh-so:P46 ;
                sh:name "has legal quality acquisition type";
                sh:class sdh-so:C30 ;
                sh:maxCount 1 ;
                ];
                    
        sh:property [
                sh:path sdh-so:P62 ;
                sh:name "is in relation to";
                sh:class crm:E21 ;
                
                ];
                    
        sh:property [
                sh:path sdh-so:P62 ;
                sh:name "is in relation to";
                sh:class crm:E74 ;
                
                ];
