sdh-shacl:crm_E12_Shape a sh:NodeShape ;
    sh:targetClass crm:E12 ;
    sh:name "Production" ;

    sh:property [
        sh:path rdfs:label ;
        sh:name "Label";
        sh:datatype xsd:string;
        sh:order 1;
        
        sh:maxCount 1;
        ];

    sh:property [
        sh:path rdfs:comment ;
        sh:name "Description";
        sh:datatype rdf:HTML;
        sh:order 2;
        
        ];   
    
    
