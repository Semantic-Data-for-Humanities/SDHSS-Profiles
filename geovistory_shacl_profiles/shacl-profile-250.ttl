sdh-shacl:sdh-so_C35_Shape a sh:NodeShape ;
    sh:targetClass sdh-so:C35 ;
    sh:name "Actor's Quality in Relation to an Event" ;

    sh:property [
        sh:path rdfs:label ;
        sh:name "Label";
        sh:datatype xsd:string;
        sh:order 1;
        sh:minCount 1;
        sh:maxCount 1;
        ];

    sh:property [
        sh:path rdfs:comment ;
        sh:name "Description";
        sh:datatype rdf:HTML;
        sh:order 2;
        sh:minCount 1;
        ];   
    
       
            
        sh:property [
                sh:path sdh-so:P57 ;
                sh:name "is quality in relation to";
                sh:class frbroo:F28 ;
                sh:maxCount 1 ;
                ];
                    
        sh:property [
                sh:path sdh-so:P58 ;
                sh:name "is about";
                sh:class sdh-so:C2 ;
                
                ];
                    
        sh:property [
                sh:path sdh-so:P59 ;
                sh:name "is quality of";
                sh:class crm:E21 ;
                sh:maxCount 1 ;
                ];
                    
        sh:property [
                sh:path sdh-so:P59 ;
                sh:name "is quality of";
                sh:class crm:E74 ;
                sh:maxCount 1 ;
                ];
                    
        sh:property [
                sh:path sdh-so:P61 ;
                sh:name "is quality on behalf of";
                sh:class crm:E21 ;
                
                ];
                    
        sh:property [
                sh:path sdh-so:P61 ;
                sh:name "is quality on behalf of";
                sh:class crm:E74 ;
                
                ];
