sdh-shacl:sdh-int_C9_Shape a sh:NodeShape ;
    sh:targetClass sdh-int:C9 ;
    sh:name "Asserted Actor's Role" ;

    sh:property [
        sh:path rdfs:label ;
        sh:name "Label";
        sh:datatype xsd:string;
        sh:order 1;
        sh:minCount 1;
        sh:maxCount 1;
        ];

    sh:property [
        sh:path rdfs:comment ;
        sh:name "Description";
        sh:datatype rdf:HTML;
        sh:order 2;
        sh:minCount 1;
        ];   
    
       
            
        sh:property [
                sh:path sdh-int:P15 ;
                sh:name "is role of";
                sh:class crm:E21 ;
                sh:maxCount 1 ;
                ];
                    
        sh:property [
                sh:path sdh-int:P15 ;
                sh:name "is role of";
                sh:class crm:E40 ;
                sh:maxCount 1 ;
                ];
                    
        sh:property [
                sh:path sdh-int:P15 ;
                sh:name "is role of";
                sh:class crm:E74 ;
                sh:maxCount 1 ;
                ];
                    
        sh:property [
                sh:path sdh-int:P16 ;
                sh:name "is asserted by";
                sh:class frbroo:F2 ;
                sh:maxCount 1 ;
                ];
                    
        sh:property [
                sh:path sdh-int:P16 ;
                sh:name "is asserted by";
                sh:class sdh-int:C2 ;
                sh:maxCount 1 ;
                ];
                    
        sh:property [
                sh:path sdh-int:P17 ;
                sh:name "is qualified by";
                sh:class sdh-so:C2 ;
                sh:maxCount 1 ;
                ];
                    
        sh:property [
                sh:path sdh-int:P18 ;
