sdh-shacl:sdh-so_C13_Shape a sh:NodeShape ;
    sh:targetClass sdh-so:C13 ;
    sh:name "Social Role Embodiment" ;

    sh:property [
        sh:path rdfs:label ;
        sh:name "Label";
        sh:datatype xsd:string;
        sh:order 1;
        
        sh:maxCount 1;
        ];

    sh:property [
        sh:path rdfs:comment ;
        sh:name "Description";
        sh:datatype rdf:HTML;
        sh:order 2;
        
        ];   
    
    
                sh:property [
                sh:path sdh-short:P1 ;
                sh:name "Standard Date-Time";
                sh:datatype xsd:string;
                sh:order 3;
                ];
                    
                
        sh:property [
                sh:path sdh-so:P26 ;
                sh:name "is embodiment by";
                sh:class crm:E21 ;
                sh:maxCount 1 ;
                ];
                    
        sh:property [
                sh:path sdh-so:P33 ;
                sh:name "is embodiment of";
                sh:class sdh-edu:C6 ;
                sh:maxCount 1 ;
                ];
                    
        sh:property [
                sh:path sdh-so:P56 ;
                sh:name "carried out in the context of";
                sh:class crm:E74 ;
                sh:maxCount 1 ;
                ];
                . 

