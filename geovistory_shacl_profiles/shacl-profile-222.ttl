sdh-shacl:sdh-edu_C2_Shape a sh:NodeShape ;
    sh:targetClass sdh-edu:C2 ;
    sh:name "Study" ;

    sh:property [
        sh:path rdfs:label ;
        sh:name "Label";
        sh:datatype xsd:string;
        sh:order 1;
        
        sh:maxCount 1;
        ];

    sh:property [
        sh:path rdfs:comment ;
        sh:name "Description";
        sh:datatype rdf:HTML;
        sh:order 2;
        
        ];   
    
    
                sh:property [
                sh:path sdh-short:P1 ;
                sh:name "Standard Date-Time";
                sh:datatype xsd:string;
                sh:maxCount 1;
                sh:order 3;
                ];
                    
                
        sh:property [
                sh:path sdh:P6 ;
                sh:name "took place at";
                sh:class sdh:C13 ;
                
                ];
                    
        sh:property [
                sh:path sdh-edu:P1 ;
                sh:name "is study at";
                sh:class crm:E74 ;
                sh:maxCount 1 ;
                ];
                    
        sh:property [
                sh:path sdh-edu:P2 ;
                sh:name "is the study by";
                sh:class crm:E21 ;
                sh:maxCount 1 ;
                ];
                    
        sh:property [
                sh:path sdh-edu:P12 ;
                sh:name "is study of";
                sh:class sdh-edu:C5 ;
                
                ];
                . 

