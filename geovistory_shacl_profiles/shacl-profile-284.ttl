### SHACL OntoME Profile 284 



@prefix sdh-shacl: <https://sdhss.org/shacl/profiles/>. 
@prefix sh: <http://www.w3.org/ns/shacl#> .  
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: 	<http://www.w3.org/2000/01/rdf-schema#> .  
@prefix sdh-short: <https://sdhss.org/ontology/shortcuts/P1>.
        @prefix sdh-so: <https://ontome.net/ns/social-legal-economic-life/>.
@prefix crm: <http://www.cidoc-crm.org/cidoc-crm/>.
@prefix sdh: <https://sdhss.org/ontology/core/>. 

sdh-shacl:crm_E5_Shape a sh:NodeShape ;
    sh:targetClass crm:E5 ;
    sh:name "Event" ;

    sh:property [
        sh:path rdfs:label ;
        sh:name "Label";
        sh:datatype xsd:string;
        sh:order 1;
        
        sh:maxCount 1;
        ];

    sh:property [
        sh:path rdfs:comment ;
        sh:name "Description";
        sh:datatype rdf:HTML;
        sh:order 2;
        
        ];   
    
    
                sh:property [
                sh:path sdh-short:P1 ;
                sh:name "Standard Date-Time";
                sh:datatype xsd:string;
                sh:order 3;
                ];
                    
            . 

sdh-shacl:sdh-so_C29_Shape a sh:NodeShape ;
    sh:targetClass sdh-so:C29 ;
    sh:name "Legal Quality Acquisition" ;

    sh:property [
        sh:path rdfs:label ;
        sh:name "Label";
        sh:datatype xsd:string;
        sh:order 1;
        
        sh:maxCount 1;
        ];

    sh:property [
        sh:path rdfs:comment ;
        sh:name "Description";
        sh:datatype rdf:HTML;
        sh:order 2;
        
        ];   
    
    
                sh:property [
                sh:path sdh-short:P1 ;
                sh:name "Standard Date-Time";
                sh:datatype xsd:string;
                sh:order 3;
                ];
                    
                
        sh:property [
                sh:path sdh:P43 ;
                sh:name "has setting";
                sh:class crm:E5 ;
                
                ];
                .