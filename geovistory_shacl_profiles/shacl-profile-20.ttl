sdh-shacl:crm_E12_Shape a sh:NodeShape ;
    sh:targetClass crm:E12 ;
    sh:name "Production" ;

    sh:property [
        sh:path rdfs:label ;
        sh:name "Label";
        sh:datatype xsd:string;
        sh:order 1;
        
        sh:maxCount 1;
        ];

    sh:property [
        sh:path rdfs:comment ;
        sh:name "Description";
        sh:datatype rdf:HTML;
        sh:order 2;
        
        ];   
    
    
                sh:property [
                sh:path sdh-short:P1 ;
                sh:name "Standard Date-Time";
                sh:datatype xsd:string;
                sh:order 3;
                ];
                    
                
        sh:property [
                sh:path crm:P14 ;
                sh:name "carried out by";
                sh:class crm:E21 ;
                
                ];
                    
        sh:property [
                sh:path crm:P14 ;
