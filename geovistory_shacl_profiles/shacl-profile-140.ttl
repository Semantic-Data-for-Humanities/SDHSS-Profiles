### SHACL OntoME Profile 140 



@prefix sdh-shacl: <https://sdhss.org/shacl/profiles/>. 
@prefix sh: <http://www.w3.org/ns/shacl#> .  
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: 	<http://www.w3.org/2000/01/rdf-schema#> .  
@prefix sdh-short: <https://sdhss.org/ontology/shortcuts/>.
        @prefix sdh: <https://sdhss.org/ontology/core/>.
@prefix crm: <http://www.cidoc-crm.org/cidoc-crm/>.
@prefix sdh-so: <https://ontome.net/ns/social-legal-economic-life/>. 

sdh-shacl:crm_E8_Shape a sh:NodeShape ;
    sh:targetClass crm:E8 ;
    sh:name "Acquisition" ;

    sh:property [
        sh:path rdfs:label ;
        sh:name "Label";
        sh:datatype xsd:string;
        sh:order 1;
        
        sh:maxCount 1;
        ];

    sh:property [
        sh:path rdfs:comment ;
        sh:name "Description";
        sh:datatype rdf:HTML;
        sh:order 2;
        
        ];   
    
    
                sh:property [
                sh:path sdh-short:P1 ;
                sh:name "Standard Date-Time";
                sh:datatype xsd:string;
                sh:maxCount 1;
                sh:order 3;
                ];
                    
                
        sh:property [
                sh:path crm:P8 ;
                sh:name "took place on or within";
                sh:class crm:E18 ;
                
                ];
                    
        sh:property [
                sh:path crm:P22 ;
                sh:name "transferred title to";
                sh:class crm:E21 ;
                
                ];
                    
        sh:property [
                sh:path crm:P22 ;
                sh:name "transferred title to";
                sh:class crm:E74 ;
                
                ];
                    
        sh:property [
                sh:path crm:P23 ;
                sh:name "transferred title from";
                sh:class crm:E21 ;
                
                ];
                    
        sh:property [
                sh:path crm:P23 ;
                sh:name "transferred title from";
                sh:class crm:E74 ;
                
                ];
                    
        sh:property [
                sh:path crm:P24 ;
                sh:name "transferred title of";
                sh:class crm:E24 ;
                
                ];
                . 

