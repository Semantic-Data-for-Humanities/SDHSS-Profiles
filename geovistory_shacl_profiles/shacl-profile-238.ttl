### SHACL OntoME Profile 238 



@prefix sdh-shacl: <https://sdhss.org/shacl/profiles/>. 
@prefix sh: <http://www.w3.org/ns/shacl#> .  
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: 	<http://www.w3.org/2000/01/rdf-schema#> .  
@prefix sdh-short: <https://sdhss.org/ontology/shortcuts/>.
        @prefix sdh: <https://sdhss.org/ontology/core/>.
@prefix crm: <http://www.cidoc-crm.org/cidoc-crm/>.
@prefix crm-sup: <https://sdhss.org/ontology/crm-supplement/>. 

sdh-shacl:sdh_C70_Shape a sh:NodeShape ;
    sh:targetClass sdh:C70 ;
    sh:name "Group Classification" ;

    sh:property [
        sh:path rdfs:label ;
        sh:name "Label";
        sh:datatype xsd:string;
        sh:order 1;
        sh:minCount 1;
        sh:maxCount 1;
        ];

    sh:property [
        sh:path rdfs:comment ;
        sh:name "Description";
        sh:datatype rdf:HTML;
        sh:order 2;
        sh:minCount 1;
        ];   
    
       
            
        sh:property [
                sh:path sdh:P7 ;
                sh:name "is intention of";
                sh:class crm:E21 ;
                sh:maxCount 1 ;
                ];
                    
        sh:property [
                sh:path sdh:P7 ;
                sh:name "is intention of";
                sh:class crm:E74 ;
                sh:maxCount 1 ;
                ];
                    
        sh:property [
                sh:path sdh:P87 ;
                sh:name "classifies with";
                sh:class crm-sup:C9 ;
                sh:maxCount 1 ;
                ];
                    
        sh:property [
                sh:path sdh:P88 ;
                sh:name "classifies";
                sh:class crm:E74 ;
                sh:maxCount 1 ;
                ];
                .