sdh-shacl:crm_E65_Shape a sh:NodeShape ;
    sh:targetClass crm:E65 ;
    sh:name "Creation" ;

    sh:property [
        sh:path rdfs:label ;
        sh:name "Label";
        sh:datatype xsd:string;
        sh:order 1;
        
        sh:maxCount 1;
        ];

    sh:property [
        sh:path rdfs:comment ;
        sh:name "Description";
        sh:datatype rdf:HTML;
        sh:order 2;
        
        ];   
    
    
                sh:property [
                sh:path sdh-short:P1 ;
                sh:name "Standard Date-Time";
                sh:datatype xsd:string;
                sh:order 3;
                ];
                    
                
        sh:property [
                sh:path crm:P4 ;
                sh:name "has time-span";
                sh:class crm:E52 ;
                sh:maxCount 1 ;
                ];
                    
        sh:property [
                sh:path crm:P94 ;
                sh:name "has created";
                sh:class crm:E33 ;
                
                ];
                    
        sh:property [
                sh:path sdh:P6 ;
                sh:name "took place at";
                sh:class sdh:C13 ;
                
                ];
                . 

