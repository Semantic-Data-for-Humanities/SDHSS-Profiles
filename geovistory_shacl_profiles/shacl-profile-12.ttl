sdh-shacl:crm_E21_Shape a sh:NodeShape ;
    sh:targetClass crm:E21 ;
    sh:name "Person" ;

    sh:property [
        sh:path rdfs:label ;
        sh:name "Label";
        sh:datatype xsd:string;
        sh:order 1;
        sh:minCount 1;
        sh:maxCount 1;
        ];

    sh:property [
        sh:path rdfs:comment ;
        sh:name "Description";
        sh:datatype rdf:HTML;
        sh:order 2;
        sh:minCount 1;
        ];   
    
            sh:property [
                sh:path sdh-short:P2 ;
                sh:name "has birth date";
                sh:datatype xsd:string;
                sh:maxCount 1;
                sh:order 3;
                ];
                
        
                
        sh:property [
                sh:path sdh-so:P23 ;
                sh:name "has gender";
                sh:class sdh-so:C11 ;
                
                ];
                    
        sh:property [
                sh:path sdh-so:P24 ;
                sh:name "has its origins in";
                sh:class sdh:C13 ;
                
                ];
                . 

