### SHACL OntoME Profile 113 



@prefix sdh-shacl: <https://sdhss.org/shacl/profiles/>. 
@prefix sh: <http://www.w3.org/ns/shacl#> .  
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: 	<http://www.w3.org/2000/01/rdf-schema#> .  
@prefix sdh-short: <https://sdhss.org/ontology/shortcuts/P1>.
        @prefix crm: <http://www.cidoc-crm.org/cidoc-crm/>.
@prefix sdh: <https://sdhss.org/ontology/core/>.
@prefix crm-sup: <https://sdhss.org/ontology/crm-supplement/>. 

sdh-shacl:crm_E7_Shape a sh:NodeShape ;
    sh:targetClass crm:E7 ;
    sh:name "Activity" ;

    sh:property [
        sh:path rdfs:label ;
        sh:name "Label";
        sh:datatype xsd:string;
        sh:order 1;
        
        sh:maxCount 1;
        ];

    sh:property [
        sh:path rdfs:comment ;
        sh:name "Description";
        sh:datatype rdf:HTML;
        sh:order 2;
        
        ];   
    
    
                sh:property [
                sh:path sdh-short:P1 ;
                sh:name "Standard Date-Time";
                sh:datatype xsd:string;
                sh:order 3;
                ];
                    
                
        sh:property [
                sh:path crm:P14 ;
                sh:name "carried out by";
                sh:class crm:E21 ;
                
                ];
                    
        sh:property [
                sh:path crm:P14 ;
                sh:name "carried out by";
                sh:class crm:E74 ;
                
                ];
                    
        sh:property [
                sh:path crm:P20 ;
                sh:name "had specific purpose";
                sh:class crm:E7 ;
                
                ];
                    
        sh:property [
                sh:path crm:P134 ;
                sh:name "continued";
                sh:class crm:E7 ;
                
                ];
                    
        sh:property [
                sh:path sdh:P6 ;
                sh:name "took place at";
                sh:class sdh:C13 ;
                
                ];
                    
        sh:property [
                sh:path crm-sup:P19 ;
                sh:name "has activity type";
                sh:class crm-sup:C3 ;
                sh:maxCount 1 ;
                ];
                . 

sdh-shacl:crm_E21_Shape a sh:NodeShape ;
    sh:targetClass crm:E21 ;
    sh:name "Person" ;

    sh:property [
        sh:path rdfs:label ;
        sh:name "Label";
        sh:datatype xsd:string;
        sh:order 1;
        sh:minCount 1;
        sh:maxCount 1;
        ];

    sh:property [
        sh:path rdfs:comment ;
        sh:name "Description";
        sh:datatype rdf:HTML;
        sh:order 2;
        sh:minCount 1;
        ];   
    
            sh:property [
                sh:path sdh-short:P2 ;
                sh:name "has birth date";
                sh:datatype xsd:string;
                sh:order 3;
                ];
                
        
            . 

