### SHACL OntoME Profile 105 



        @prefix sdh-shacl: <https://sdhss.org/shacl/profiles/>. 
        @prefix sh: <http://www.w3.org/ns/shacl#> .  
        @prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
        @prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
        @prefix rdfs: 	<http://www.w3.org/2000/01/rdf-schema#> .  
        @prefix sdh-short: <https://sdhss.org/ontology/shortcuts/P1>.
        @prefix crm: <http://www.cidoc-crm.org/cidoc-crm/>.
@prefix sdh-int: <https://ontome.net/ns/intellectual-literary-life/>.
@prefix sdh: <https://sdhss.org/ontology/core/>. 

        sdh-shacl:crm_E21_Shape a sh:NodeShape ;
            sh:targetClass crm:E21 ;
            sh:name "Person" ;

            sh:property [
                sh:path rdfs:label ;
                sh:name "Label";
                sh:datatype xsd:string;
                sh:order 1;
                sh:minCount 1;
                sh:maxCount 1;
                ];

            sh:property [
                sh:path rdfs:comment ;
                sh:name "Description";
                sh:datatype rdf:HTML;
                sh:order 2;
                sh:minCount 1;
                ];   
            
            sh:property [
                sh:path sdh-short:P2 ;
                sh:name "has birth date";
                sh:datatype xsd:string;
                sh:order 3;
                ];
                
                
                
        sh:property [
                sh:path sdh-int:P21 ;
                sh:name "effects dedication";
                sh:class sdh-int:C12 ;
                
                ];
                . 

        sdh-shacl:crm_E24_Shape a sh:NodeShape ;
            sh:targetClass crm:E24 ;
            sh:name "Physical Human-Made Thing" ;

            sh:property [
                sh:path rdfs:label ;
                sh:name "Label";
                sh:datatype xsd:string;
                sh:order 1;
                sh:minCount 1;
                sh:maxCount 1;
                ];

            sh:property [
                sh:path rdfs:comment ;
                sh:name "Description";
                sh:datatype rdf:HTML;
                sh:order 2;
                sh:minCount 1;
                ];   
            
                
            . 

        sdh-shacl:crm_E74_Shape a sh:NodeShape ;
            sh:targetClass crm:E74 ;
            sh:name "Group" ;

            sh:property [
                sh:path rdfs:label ;
                sh:name "Label";
                sh:datatype xsd:string;
                sh:order 1;
                sh:minCount 1;
                sh:maxCount 1;
                ];

            sh:property [
                sh:path rdfs:comment ;
                sh:name "Description";
                sh:datatype rdf:HTML;
                sh:order 2;
                sh:minCount 1;
                ];   
            
                
                
        sh:property [
                sh:path sdh-int:P21 ;
                sh:name "effects dedication";
                sh:class sdh-int:C12 ;
                
                ];
                . 

        sdh-shacl:sdh_C32_Shape a sh:NodeShape ;
            sh:targetClass sdh:C32 ;
            sh:name "Abstract individual" ;

            sh:property [
                sh:path rdfs:label ;
                sh:name "Label";
                sh:datatype xsd:string;
                sh:order 1;
                sh:minCount 1;
                sh:maxCount 1;
                ];

            sh:property [
                sh:path rdfs:comment ;
                sh:name "Description";
                sh:datatype rdf:HTML;
                sh:order 2;
                sh:minCount 1;
                ];   
            
                
            . 

        sdh-shacl:sdh-int_C12_Shape a sh:NodeShape ;
            sh:targetClass sdh-int:C12 ;
            sh:name "Dedication" ;

            sh:property [
                sh:path rdfs:label ;
                sh:name "Label";
                sh:datatype xsd:string;
                sh:order 1;
                
                sh:maxCount 1;
                ];

            sh:property [
                sh:path rdfs:comment ;
                sh:name "Description";
                sh:datatype rdf:HTML;
                sh:order 2;
                
                ];   
            
            
                sh:property [
                sh:path sdh-short:P1 ;
                sh:name "Standard Date-Time";
                sh:datatype xsd:string;
                sh:order 3;
                ];
                    
                
        sh:property [
                sh:path sdh-int:P22 ;
                sh:name "has dedicatory object";
                sh:class crm:E24 ;
                
                ];
                    
        sh:property [
                sh:path sdh-int:P23 ;
                sh:name "has dedicatee";
                sh:class crm:E21 ;
                
                ];
                    
        sh:property [
                sh:path sdh-int:P23 ;
                sh:name "has dedicatee";
                sh:class crm:E74 ;
                
                ];
                    
        sh:property [
                sh:path sdh-int:P24 ;
                sh:name "has conceptual dedicatee";
                sh:class sdh:C32 ;
                
                ];
                    
        sh:property [
                sh:path sdh-int:P25 ;
                sh:name "has beneficiary";
                sh:class crm:E21 ;
                
                ];
                    
        sh:property [
                sh:path sdh-int:P25 ;
                sh:name "has beneficiary";
                sh:class crm:E74 ;
                
                ];
                .