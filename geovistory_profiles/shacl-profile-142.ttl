        sdh-shacl:crm_E54_Shape a sh:NodeShape ;
            sh:targetClass crm:E54 ;
            sh:name "Dimension" ;

            sh:property [
                sh:path rdfs:label ;
                sh:name "Label";
                sh:datatype xsd:string;
                sh:order 1;
                sh:minCount 1;
                sh:maxCount 1;
                ];

            sh:property [
                sh:path rdfs:comment ;
                sh:name "Description";
                sh:datatype rdf:HTML;
                sh:order 2;
                sh:minCount 1;
                ];   
            
                
                
        sh:property [
                sh:path crm:P90 ;
                sh:name "has value";
                sh:class crm:E60 ;
                sh:maxCount 1 ;
                ];
                    
        sh:property [
                sh:path crm:P91 ;
                sh:name "has unit";
                sh:class crm:E58 ;
                sh:maxCount 1 ;
                ];
                    
        sh:property [
                sh:path crm-sup:P25 ;
                sh:name "has dimension type";
                sh:class crm-sup:C27 ;
                sh:maxCount 1 ;
                ];
                . 

