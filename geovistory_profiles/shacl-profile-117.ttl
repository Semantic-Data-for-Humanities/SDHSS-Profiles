### SHACL OntoME Profile 117 



        @prefix sdh-shacl: <https://sdhss.org/shacl/profiles/>. 
        @prefix sh: <http://www.w3.org/ns/shacl#> .  
        @prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
        @prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
        @prefix rdfs: 	<http://www.w3.org/2000/01/rdf-schema#> .  
        @prefix sdh-short: <https://sdhss.org/ontology/shortcuts/P1>.
        @prefix crmsci: <http://www.cidoc-crm.org/cidoc-crm/CRMsci/>.
@prefix sdh: <https://sdhss.org/ontology/core/>.
@prefix crm-sup: <https://sdhss.org/ontology/crm-supplement/>. 

        sdh-shacl:sdh_C13_Shape a sh:NodeShape ;
            sh:targetClass sdh:C13 ;
            sh:name "Geographical Place" ;

            sh:property [
                sh:path rdfs:label ;
                sh:name "Label";
                sh:datatype xsd:string;
                sh:order 1;
                sh:minCount 1;
                sh:maxCount 1;
                ];

            sh:property [
                sh:path rdfs:comment ;
                sh:name "Description";
                sh:datatype rdf:HTML;
                sh:order 2;
                sh:minCount 1;
                ];   
            
                
            . 

        sdh-shacl:crmsci_S4_Shape a sh:NodeShape ;
            sh:targetClass crmsci:S4 ;
            sh:name "Observation" ;

            sh:property [
                sh:path rdfs:label ;
                sh:name "Label";
                sh:datatype xsd:string;
                sh:order 1;
                
                sh:maxCount 1;
                ];

            sh:property [
                sh:path rdfs:comment ;
                sh:name "Description";
                sh:datatype rdf:HTML;
                sh:order 2;
                
                ];   
            
            
                sh:property [
                sh:path sdh-short:P1 ;
                sh:name "Standard Date-Time";
                sh:datatype xsd:string;
                sh:order 3;
                ];
                    
                
        sh:property [
                sh:path crmsci:O9 ;
                sh:name "observed property type";
                sh:class crmsci:S9 ;
                
                ];
                    
        sh:property [
                sh:path sdh:P6 ;
                sh:name "took place at";
                sh:class sdh:C13 ;
                
                ];
                    
        sh:property [
                sh:path crm-sup:P30 ;
                sh:name "observed entity type";
                sh:class crm-sup:C32 ;
                
                ];
                . 

        sdh-shacl:crmsci_S9_Shape a sh:NodeShape ;
            sh:targetClass crmsci:S9 ;
            sh:name "Property Type" ;

            sh:property [
                sh:path rdfs:label ;
                sh:name "Label";
                sh:datatype xsd:string;
                sh:order 1;
                sh:minCount 1;
                sh:maxCount 1;
                ];

            sh:property [
                sh:path rdfs:comment ;
                sh:name "Description";
                sh:datatype rdf:HTML;
                sh:order 2;
                sh:minCount 1;
                ];   
            
                
            . 

        sdh-shacl:crm-sup_C32_Shape a sh:NodeShape ;
            sh:targetClass crm-sup:C32 ;
            sh:name "Observed Entity Type" ;

            sh:property [
                sh:path rdfs:label ;
                sh:name "Label";
                sh:datatype xsd:string;
                sh:order 1;
                sh:minCount 1;
                sh:maxCount 1;
                ];

            sh:property [
                sh:path rdfs:comment ;
                sh:name "Description";
                sh:datatype rdf:HTML;
                sh:order 2;
                sh:minCount 1;
                ];   
            
                
            .