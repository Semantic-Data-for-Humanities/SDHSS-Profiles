### SHACL OntoME Profile 103 



        @prefix sdh-shacl: <https://sdhss.org/shacl/profiles/>. 
        @prefix sh: <http://www.w3.org/ns/shacl#> .  
        @prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
        @prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
        @prefix rdfs: 	<http://www.w3.org/2000/01/rdf-schema#> .  
        @prefix sdh-short: <https://sdhss.org/ontology/shortcuts/P1>.
        @prefix crm: <http://www.cidoc-crm.org/cidoc-crm/>.
@prefix sdh: <https://sdhss.org/ontology/core/>.
@prefix crm-sup: <https://sdhss.org/ontology/crm-supplement/>. 

        sdh-shacl:crm_E21_Shape a sh:NodeShape ;
            sh:targetClass crm:E21 ;
            sh:name "Person" ;

            sh:property [
                sh:path rdfs:label ;
                sh:name "Label";
                sh:datatype xsd:string;
                sh:order 1;
                sh:minCount 1;
                sh:maxCount 1;
                ];

            sh:property [
                sh:path rdfs:comment ;
                sh:name "Description";
                sh:datatype rdf:HTML;
                sh:order 2;
                sh:minCount 1;
                ];   
            
            sh:property [
                sh:path sdh-short:P2 ;
                sh:name "has birth date";
                sh:datatype xsd:string;
                sh:order 3;
                ];
                
                
            . 

        sdh-shacl:crm_E24_Shape a sh:NodeShape ;
            sh:targetClass crm:E24 ;
            sh:name "Physical Human-Made Thing" ;

            sh:property [
                sh:path rdfs:label ;
                sh:name "Label";
                sh:datatype xsd:string;
                sh:order 1;
                sh:minCount 1;
                sh:maxCount 1;
                ];

            sh:property [
                sh:path rdfs:comment ;
                sh:name "Description";
                sh:datatype rdf:HTML;
                sh:order 2;
                sh:minCount 1;
                ];   
            
                
                
        sh:property [
                sh:path crm:P128 ;
                sh:name "carries";
                sh:class crm:E73 ;
                
                ];
                . 

        sdh-shacl:crm_E73_Shape a sh:NodeShape ;
            sh:targetClass crm:E73 ;
            sh:name "Information Object" ;

            sh:property [
                sh:path rdfs:label ;
                sh:name "Label";
                sh:datatype xsd:string;
                sh:order 1;
                sh:minCount 1;
                sh:maxCount 1;
                ];

            sh:property [
                sh:path rdfs:comment ;
                sh:name "Description";
                sh:datatype rdf:HTML;
                sh:order 2;
                sh:minCount 1;
                ];   
            
                
                
        sh:property [
                sh:path crm-sup:P26 ;
                sh:name "has information object type";
                sh:class crm-sup:C28 ;
                sh:maxCount 1 ;
                ];
                . 

        sdh-shacl:sdh_C32_Shape a sh:NodeShape ;
            sh:targetClass sdh:C32 ;
            sh:name "Abstract individual" ;

            sh:property [
                sh:path rdfs:label ;
                sh:name "Label";
                sh:datatype xsd:string;
                sh:order 1;
                sh:minCount 1;
                sh:maxCount 1;
                ];

            sh:property [
                sh:path rdfs:comment ;
                sh:name "Description";
                sh:datatype rdf:HTML;
                sh:order 2;
                sh:minCount 1;
                ];   
            
                
            . 

        sdh-shacl:crm-sup_C28_Shape a sh:NodeShape ;
            sh:targetClass crm-sup:C28 ;
            sh:name "Information Object Type" ;

            sh:property [
                sh:path rdfs:label ;
