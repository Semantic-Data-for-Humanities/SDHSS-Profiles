            sh:name "Physical Human-Made Thing" ;

            sh:property [
                sh:path rdfs:label ;
                sh:name "Label";
                sh:datatype xsd:string;
                sh:order 1;
                sh:minCount 1;
                sh:maxCount 1;
                ];

            sh:property [
                sh:path rdfs:comment ;
                sh:name "Description";
                sh:datatype rdf:HTML;
                sh:order 2;
                sh:minCount 1;
                ];   
            
                
            . 

